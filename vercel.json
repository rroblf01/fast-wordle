{
    "devCommand": "uvicorn src:index --host 0.0.0.0 --port 3000",
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }